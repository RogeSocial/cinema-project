    "roles":[
      {"role": "user", "methods": ["GET"]}
    ]
  },

  {
    "route": "/rest/video_example",
    "roles":[
      {"role": "*", "methods": ["GET"]}      
    ]
  },

  {
    "route": "/rest/audio_streams*",
    "roles":[
      {"role": "*", "methods": ["GET"]}
    ]
  },

